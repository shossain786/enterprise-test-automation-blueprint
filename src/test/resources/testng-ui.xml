<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="UI Test Suite" parallel="methods" thread-count="2">
    <listeners>
        <listener class-name="com.utem.reporter.testng.UtemTestNGListener"/>
        <listener class-name="framework.ui.utils.DriverQuitListener"/>
    </listeners>
    <test name="UI Tests">
        <groups>
            <run>
                <include name="ui"/>
            </run>
        </groups>
        <packages>
            <package name="framework.ui.tests"/>
        </packages>
    </test>
</suite>