<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<!-- CI variant: UTEM listener excluded (server is local-only).
     Use testng-ui.xml for local runs with UTEM reporting enabled. -->
<suite name="UI Test Suite">
    <listeners>
        <listener class-name="framework.ui.utils.DriverQuitListener"/>
    </listeners>
    <test name="UI Tests">
        <groups>
            <run>
                <include name="ui"/>
            </run>
        </groups>
        <packages>
            <package name="framework.ui.tests"/>
        </packages>
    </test>
</suite>